#include<stdio.h>
int main()
{
    int n;
    printf("Array Size : ");
    scanf("%d",&n);
    printf("Enter %d Array Elements : ",n);
    int a[n],count=0;
    for( int i = 0 ; i < n; i++)
    {

        scanf("%d",&a[i]);
    }
    for( int i = 0 ; i < n ; i+=2)
    {
        count+=a[i];
    }
    printf("%d",count);
}
